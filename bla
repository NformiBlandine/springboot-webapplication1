pipeline {
    agent {
        label 'slave1'
    }
    tools{
        maven 'maven9'
    }
    stages {
        stage('checkout') {
            steps {
                git branch: 'main', url: 'https://github.com/NformiBlandine/springboot-webapplication1.git'
            }
        }
        
        stage('build') {
            steps {
                sh 'mvn clean package'
            }
        }
    }
}
