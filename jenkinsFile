pipeline {
    agent {
        label 'slave1'
    }
    
    tools {
        maven 'maven3.9'
    }

    stages {
        stage('git checkout') {
            steps {
                git branch: 'main', url: 'https://github.com/NformiBlandine/springboot-webapplication1.git'
            }
        }
        
        stage('maven build') {
            steps {
                sh 'mvn clean package'
            }
        }
        
    }
}
